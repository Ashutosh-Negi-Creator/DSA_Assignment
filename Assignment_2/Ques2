// Transpose of matrix

#include<iostream>
using namespace std;
#include <ctime>
#include <cstdlib>

int main(){
    
    srand(time(0));
    int N;
    cout<<"Enter szie"<<endl;
    cin>>N;
    int arr[N][N];
    for(int i=0;i<N;i++){
        for(int j=0;j<N;j++){
            arr[i][j]= rand() % 9 + 1;
        }
    }
    for(int i=0;i<N;i++){
        for(int j=0; j<N; j++){
            cout<<arr[i][j]<<" ";
        }
        cout<<endl;
    }
    cout<<endl<<endl;
    for(int i=0;i<N;i++){
        for(int j=0; j<N; j++){
            cout<<arr[j][i]<<" ";
        }
        cout<<endl;
    }

    return 0;
}
