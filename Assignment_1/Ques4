#include<iostream>
#include <ctime>
#include <cstdlib>
using namespace std;

int main(){
    srand(time(0));
    cout<<"Enter the size of the Matrix :-"<<endl;
    int N;
    cin>>N;
    cout<<"Enter Character (Z/D/A/X)"<<endl;
    char ch;
    cin>>ch;
    int arr[N][N];
    for(int i=0;i<N;i++){
        for(int j=0;j<N;j++){
            arr[i][j]= rand() % 9 + 1;
        }
    }
    if(ch=='Z' || ch=='z'){
        for(int i=0;i<N;i++){
            for(int j=0;j<N;j++){
                // FOR Z
                if((i!=0 && i!=N-1) && (j!=(N-1)-i)){
                    arr[i][j]=0;
                }
            }
        }
    }
    if(ch=='D' || ch=='d'){
        for(int i=0;i<N;i++){
            for(int j=0; j<N; j++){
                if((i==0 && j== (N-1)) || (i== N-1 && j== N-1) ){
                    arr[i][j]=0;
                }
                if(i!=0 && i!=N-1 && j!=N-1 && j!=0){
                    arr[i][j]=0;
                }
            }
        }
    }
    if(ch=='A' || ch=='a'){
        int mid= N/2;
        for(int i=0;i<N;i++){
            for(int j=0;j<N;j++){
                if((i==0 && j==0) || (i==0 && j==N-1)){
                    arr[i][j]=0;
                }
                if(i!=mid && i!=0 && j!=0 && j!=N-1){
                    arr[i][j]=0;
                }
            }
        }
    }
    if(ch=='X' || ch=='x'){
        for(int i=0;i<N;i++){
            for(int j=0;j<N;j++){
                if((i!=j) && j!=(N-1)-i ){
                    arr[i][j]=0;
                }
            }
        }
    }
    for(int i=0; i<N; i++){
        for(int j=0; j<N;j++){
            cout<<arr[i][j]<<"  ";
        }
        cout<<endl;
    }
    return 0;
}
