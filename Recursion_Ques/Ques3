/*
Question: Write a recursive function to check if a given string is a palindrome.
*/

#include <iostream>
#include <string>
using namespace std;


bool pal(string str) {
    if (str.length() <= 1)
        return true;
    if (str[0] != str[str.length() - 1])
        return false;
    return pal(str.substr(1, str.length() - 2));
}

int main() {
    string str;
    getline(cin, str);
    if (pal(str))
        cout << "palindrome" << endl;
    else
        cout << "not palindrome" << endl;
    return 0;
}
